@model DynamicsNAV365_StaffPortal.Models.HumanResource.PerformanceTargetModels.EmployeePerformanceTargetLineModel
<div class="row">
    <div class="col-md-12 col-lg-12">
        <div class="panel panel-success">
            <div class="panel-heading">
                <i class="fa fa-percent fa-fw"></i> 4). End Year Evaluation
            </div>
            <table id="EndYearEvaluationLineTbl" class="table table-striped table-responsive table-condensed">
                <thead>
                    <tr>
                        <th>Agreed Performance Targets</th>
                        <th>Key Performance Indicator</th>
                        <th>Appraisee Assessment</th>
                        <th>Appraisee Comments</th>
                        <th>Appraiser Assessment</th>
                        <th>Appraiser Comments</th>
                        <th>Agreed Assesment Results</th>
                        <th>Mid-Year Comments</th>
                        <th>End-Year Assesment</th>
                        <th>End-Year Comments</th>
                        <th>.</th>
                    </tr>
                </thead>
                <tbody></tbody>
                <tfoot>

                </tfoot>
            </table>
        </div>
        <div id="EndYearEvaluationLineModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="EndYearEvaluationLineModalLabel" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button class="btn close" type="button" data-dismiss="modal"><i class="fa fa-close"></i></button>
                        <h4 id="ModeratedAppraisalLineModalLabel" class="modal-title">End Year Evaluation</h4>
                    </div>
                    <div class="modal-body">
                        @using (Html.BeginForm(null, null, FormMethod.Post, new { @id = "EndYearEvaluationLineForm", @class = "" }))
                        {
                            @Html.AntiForgeryToken()

                            if (Model != null && Model.LineErrorStatus == true)
                            {
                                <div class="alert alert-danger">
                                    <strong>Error! </strong>@Html.Raw(HttpUtility.HtmlDecode(Model.LineErrorMessage))
                                </div>
                            }

                            @Html.HiddenFor(model => model.LineNo)
                            @Html.HiddenFor(model => model.AppraisalNo)

                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group label-static">
                                        @Html.LabelFor(model => model.AgreedPerformanceTargets, htmlAttributes: new { @id = "AgreedPerformanceTargetsLbl", @class = "control-label" })
                                        @Html.EditorFor(model => model.AgreedPerformanceTargets, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                                        @Html.ValidationMessageFor(model => model.AgreedPerformanceTargets, "", new { @id = "AgreedPerformanceTargetsError", @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group label-static">
                                        @Html.LabelFor(model => model.KeyPerformanceIndicator, htmlAttributes: new { @id = "KeyPerformanceIndicatorLbl", @class = "control-label" })
                                        @Html.EditorFor(model => model.KeyPerformanceIndicator, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                                        @Html.ValidationMessageFor(model => model.KeyPerformanceIndicator, "", new { @id = "KeyPerformanceIndicatorError", @class = "text-danger" })
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group label-static">
                                        @Html.LabelFor(model => model.SelfAssessment, htmlAttributes: new { @id = "SelfAssessmentLbl", @class = "control-label" })
                                        @Html.EditorFor(model => model.SelfAssessment, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                                        @Html.ValidationMessageFor(model => model.SelfAssessment, "", new { @id = "SelfAssessmentError", @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group label-static">
                                        @Html.LabelFor(model => model.AppraiseeComments, htmlAttributes: new { @id = "AppraiseeCommentsLbl", @class = "control-label" })
                                        @Html.EditorFor(model => model.AppraiseeComments, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                                        @Html.ValidationMessageFor(model => model.AppraiseeComments, "", new { @id = "AppraiseeCommentsError", @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group label-static">
                                        @Html.LabelFor(model => model.SupervisorAssessment, htmlAttributes: new { @id = "SupervisorAssessmentLbl", @class = "control-label" })
                                        @Html.EditorFor(model => model.SupervisorAssessment, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                                        @Html.ValidationMessageFor(model => model.SupervisorAssessment, "", new { @id = "SupervisorAssessmentError", @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group label-static">
                                        @Html.LabelFor(model => model.SupervisorComments, htmlAttributes: new { @id = "SupervisorCommentsLbl", @class = "control-label" })
                                        @Html.EditorFor(model => model.SupervisorComments, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                                        @Html.ValidationMessageFor(model => model.SupervisorComments, "", new { @id = "SupervisorCommentsError", @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group label-static">
                                        @Html.LabelFor(model => model.AgreedAssesmentResults, htmlAttributes: new { @id = "AgreedAssesmentResultsLbl", @class = "control-label" })
                                        @Html.EditorFor(model => model.AgreedAssesmentResults, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                                        @Html.ValidationMessageFor(model => model.AgreedAssesmentResults, "", new { @id = "AgreedAssesmentResultsError", @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group label-static">
                                        @Html.LabelFor(model => model.CommentsAfterReview, htmlAttributes: new { @id = "CommentsAfterReviewLbl", @class = "control-label" })
                                        @Html.EditorFor(model => model.CommentsAfterReview, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                                        @Html.ValidationMessageFor(model => model.CommentsAfterReview, "", new { @id = "CommentsAfterReviewError", @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group label-static">
                                        @Html.LabelFor(model => model.EndYearAssessment, htmlAttributes: new { @id = "EndYearAssessmentLbl", @class = "control-label" })
                                        @Html.EditorFor(model => model.EndYearAssessment, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.EndYearAssessment, "", new { @id = "EndYearAssessmentError", @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group label-static">
                                        @Html.LabelFor(model => model.EndYearEvaluationComments, htmlAttributes: new { @id = "EndYearEvaluationCommentsLbl", @class = "control-label" })
                                        @Html.EditorFor(model => model.EndYearEvaluationComments, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.EndYearEvaluationComments, "", new { @id = "EndYearEvaluationCommentsError", @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                    <div class="modal-footer">
                        <button id="CreateEndYearEvaluationLineBtn" class="btn btn-xs btn-primary" type="button" onclick="CreateEndYearEvaluationLine();"><i class="fa fa-plus"></i>Save</button>
                        <button id="ModifyEndYearEvaluationLineBtn" class="btn btn-xs btn-primary" type="button" onclick="ModifyEndYearEvaluationLine();"><i class="fa fa-adjust"></i>Update</button>
                        <button class="btn btn-xs btn-danger" type="button" data-dismiss="modal"><i class="fa fa-close"></i>Cancel</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>







